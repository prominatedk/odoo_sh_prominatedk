<odoo>
	<data noupdate="0">

<record model="ir.cron" id="edi_fetch_files">
	<field name="name">EDI Fetch Files</field>
	<field name="interval_number">1</field>
	<field name="interval_type">hours</field>
	<field name="priotity">5</field>
	<field name="model_id" ref="model_easyedi_transfer"></field>
	<field name="state">code</field>
    <field name="code">model.fetch_files()</field>
    <field name="numbercall">-1</field>
    <field name="doall" eval="False"/>
</record>

<record model="ir.cron" id="edi_send_files">
	<field name="name">EDI Send Files</field>
	<field name="interval_number">1</field>
	<field name="interval_type">hours</field>
	<field name="priotity">5</field>
	<field name="model_id" ref="model_easyedi_transfer"></field>
	<field name="state">code</field>
    <field name="code">model.send_files()</field>
    <field name="numbercall">-1</field>
    <field name="doall" eval="False"/>
</record>

<record model="ir.cron" id="edi_send_pricat">
	<field name="name">EDI Send PRICAT</field>
	<field name="interval_number">14</field>
	<field name="interval_type">days</field>
	<field name="priotity">5</field>
	<field name="model_id" ref="model_easyedi_transfer"></field>
	<field name="state">code</field>
    <field name="code">model.send_pricat()</field>
    <field name="numbercall">-1</field>
    <field name="doall" eval="False"/>
</record>


	</data>
</odoo>