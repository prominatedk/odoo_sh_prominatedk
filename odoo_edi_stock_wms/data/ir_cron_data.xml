<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.cron" id="odoo_edi_stock_wms_send_invoices">
            <field name="name">FlexEDI: Send Pickings to WMS</field>
            <field name="model_id" ref="odoo_edi_stock_wms.model_flexedi_document_wms_picking"/>
            <field name="state">code</field>
            <field name="code">model._cron_process_flexedi_documents(job_count=20)</field>
            <field name="interval_number">2</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        <record model="ir.cron" id="odoo_edi_stock_wms_status_updates">
            <field name="name">FlexEDI: Check for status updates on pickings in WMS</field>
            <field name="model_id" ref="odoo_edi_stock_wms.model_flexedi_document_wms_picking"/>
            <field name="state">code</field>
            <field name="code">model._cron_get_status_update(job_count=20)</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
