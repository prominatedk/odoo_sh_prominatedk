<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.cron" id="odoo_edi_recieve_documents">
            <field name="name">FlexEDI: Recieve EDI Documents</field>
            <field name="model_id" ref="odoo_edi.model_flexedi_document"/>
            <field name="state">code</field>
            <field name="code">model._cron_receive_flexedi_documents(job_count=20)</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        <record model="ir.cron" id="odoo_edi_status_updates">
            <field name="name">FlexEDI: Check for status updates on sent documents</field>
            <field name="model_id" ref="odoo_edi.model_flexedi_document"/>
            <field name="state">code</field>
            <field name="code">model._cron_get_status_update(job_count=20)</field>
            <field name="interval_number">30</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
